    "banner": "banner-cli dist/*.styl",
    "build": "run-s clean copy banner stylus autoprefixer",
    "clean": "rimraf dist",
    "copy": "copycopy src/**/* dist",
    "lint": "stylint --config .styluslintrc src/*styl && eslint test -c styled && editorconfig-tools check .",
    "precommit": "run-p test",
    "start": "run-p build watch",
    "stylus": "rm -rf test && mkdir test && stylus src/milligram.styl --out test/milligram.css && stylus --compress src/milligram.styl --out test/milligram.min.css",
